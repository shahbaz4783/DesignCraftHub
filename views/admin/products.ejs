<section class="admin-products">
			<% for (item of product) { %>
			<article class="card">
				<div class="imgDiv">
					<img
						src="https://images.unsplash.com/photo-1562771242-a02d9090c90c?w=800&auto=format&fit=crop&q=60&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxzZWFyY2h8Mzh8fGd5bXxlbnwwfDB8MHx8fDI%3D"
						alt=""
					/>
				</div>
				<div class="textBox">
					<div class="textContent">
						<p class="h1"><%= item.title %></p>
						<span class="span">$<%= item.price %></span>
					</div>
					<p class="p"><%= item.description %></p>
					<div>
                            <a href="/delete"><img src="/assets/delete.svg" alt=""></a>
                            <a href="/edit"><img src="/assets/edit.svg" alt=""></a>
                    </div>
				</div>
			</article>
			<% } %>
</section>

